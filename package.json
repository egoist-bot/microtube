{
  "name": "youtube-lite",
  "version": "1.0.0",
  "description": "light youtube player",
  "scripts": {
    "build": "webpack --progress -p --optimize-minimize",
    "clean": "rm public/*.hot-update.js",
    "postinstall": "npm run build",
    "watch": "webpack-dev-server --progress --colors --inline --hot",
    "test": "npm run test:client && npm run test:server",
    "test:client": "mocha test/client --recursive --compilers js:babel-register",
    "test:server": "mocha test/server --recursive --compilers js:babel-register"
  },
  "dependencies": {
    "array.prototype.move": "0.0.3",
    "firebase": "^3.6.6",
    "material-design-lite": "^1.2.0",
    "react": "^15.4.2",
    "react-cookie": "^0.4.8",
    "react-document-title": "^2.0.2",
    "react-dom": "15.1.0",
    "react-linked-input": "^1.0.1",
    "react-mixin": "^3.0.5",
    "react-redux": "4.4.1",
    "react-router": "^2.4.0",
    "react-waypoint": "^3.1.1",
    "react-youtube": "7.2.0",
    "redbox-react": "^1.2.4",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
