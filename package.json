    "lodash-webpack-plugin": "^0.11.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.0.2",
