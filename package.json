{
  "name": "youtube-lite",
  "version": "1.0.0",
  "description": "light youtube player",
  "scripts": {
    "start": "node server/app.js",
    "start:dev": "nodemon server/app.js --watch server --dev",
    "build": "webpack --progress -p --optimize-minimize",
    "clean": "rm public/*.hot-update.js",
    "postinstall": "npm run build",
    "watch": "webpack-dev-server --progress --colors --inline --hot --port 3000",
    "test": "npm run test:client && npm run test:server",
    "test:client": "mocha test/client --recursive --compilers js:babel-register",
    "test:server": "mocha test/server --recursive --compilers js:babel-register"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-rewire": "^1.0.0-rc-4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "compression": "^1.7.0",
    "compression-webpack-plugin": "^0.3.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.3.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "3.0.2",
    "file-loader": "^0.9.0",
    "lodash-webpack-plugin": "^0.11.0",
    "mocha": "^2.4.5",
    "morgan": "^1.8.1",
    "node-sass": "^4.3.0",
    "offline-plugin": "^4.8.3",
    "portfinder": "^1.0.13",
    "pug": "^2.0.0-rc.3",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.0.2",
    "react-cookie": "^0.4.8",
    "react-document-title": "^2.0.2",
    "react-fastclick-alt": "^1.1.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.4.0",
    "react-waypoint": "^3.1.1",
    "react-web-notification": "^0.2.4",
    "react-youtube": "^7.4.0",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-mock-store": "^1.0.2",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.13.0",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "uuid": "^3.0.1",
    "webpack": "^1.12.14",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.18.0",
    "write-file-webpack-plugin": "^3.1.8",
    "yargs": "^6.6.0"
  }
}
